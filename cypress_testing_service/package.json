{
  "name": "package.json",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'cypress/**/*.{js,ts,tsx}'",
    "pretty": "prettier --write \"cypress/**/*.{ts,js,jsx,json}\"",
    "cy:run": "cypress run --config video=false",
    "cy:open": "cypress open",
    "backend": "docker-compose run --rm backend",
    "initdb": "yarn backend ./manage.py initcypress --skip-drop",
    "generate-xlsx-files": "./generate_xlsx_files.sh",
    "clean": "rm -rf cypress/fixtures/*.xlsx ; rm -rf cypress/downloads/*.{xlsx,zip} ; yarn backend ./manage.py clean_generated",
    "init-scenario": "yarn backend ./manage.py init-e2e-scenario"
  },
  "devDependencies": {
    "@types/cypress-cucumber-preprocessor": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cypress-multi-reporters": "^1.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha-junit-reporter": "^1.23.3",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@cypress/browserify-preprocessor": "^2.2.4",
    "@types/faker": "^4.1.12",
    "cypress": "9.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-file-upload": "^3.5.3",
    "faker": "^4.1.0",
    "node-xlsx": "^0.21.0",
    "puppeteer": "^2.1.1",
    "tsify": "^4.0.1",
    "typescript": "^4.8.3",
    "uuidv4": "^6.0.8",
    "xlsx": "^0.16.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
